  rightArm
  {
    (0,0)   0.00       ; torso_lift_joint
    (1,0)   0.00       ; r_shoulder_pan_joint
    (2,0)   0.00     ; r_shoulder_lift_joint
    (3,0)   0.00     ; r_upper_arm_roll_joint
    (4,0)   0.000     ; r_elbow_flex_joint
    (5,0)   0.000     ; r_forearm_roll_joint
  }
}
inputCost
{
  R
  {
    base
    {
      (0,0) 15.5 ; forward velocity
      (1,1) 18.5 ; turning velocity
      (2,2) 10.5 ; turning velocity
    }
    leftarm
    {
        (0,0)  3.0  ; joint1 velocity
        (1,1)  1.0  ; joint2 velocity
        (2,2)  1.0  ; joint3 velocity
        (3,3)  1.50  ; joint4 velocity
        (4,4)  1.50  ; joint5 velocity
        (5,5)  1.0  ; joint6 velocity
    }
    rightarm
    {
        (0,0)  3.0  ; joint1 velocity
        (1,1)  1.0  ; joint2 velocity
        (2,2)  1.0  ; joint3 velocity
        (3,3)  1.50  ; joint4 velocity
        (4,4)  1.50  ; joint5 velocity
        (5,5)  1.0  ; joint6 velocity
    }
  }
