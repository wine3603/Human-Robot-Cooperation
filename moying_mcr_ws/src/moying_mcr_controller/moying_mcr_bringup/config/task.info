    (1,0)   0.00       ; r_shoulder_pan_joint
    (2,0)   0.00     ; r_shoulder_lift_joint
    (3,0)   0.00     ; r_upper_arm_roll_joint
    (4,0)   0.000     ; r_elbow_flex_joint
    (5,0)   0.000     ; r_forearm_roll_joint
