endLeftEffector
{
  ; end effector quadratic penalty scaling
  muPosition      80.0
  muOrientation   40.0
}

finalEndLeftEffector
{
  muPosition      80.0
  muOrientation   40.0
}
endRightEffector
{
  ; end effector quadratic penalty scaling
  muPosition      80.0
  muOrientation    40.0
}

finalEndRightEffector
{
  muPosition      80.0
  muOrientation    40.0
}

endBase
{
  muPosition      60.0
  muOrientation   80.0
}

selfCollision
{
  ; activate self-collision constraint
  activate false

  ; Self Collision pairs
  collisionLinkPairs
  {
    [0] "right_arm_elfin_link6, left_arm_elfin_link6"
    [1] "right_arm_elfin_link4, extention_shell"
    [2] "right_arm_elfin_link2, body_base"
    [3] "left_arm_elfin_link2, body_base"
    [4] "left_arm_elfin_link4, extention_shell"
  }

  ; minimum distance allowed between the pairs
  minimumDistance  0.1

  ; relaxed log barrier mu
  mu     1e-2

  ; relaxed log barrier delta
  delta  1e-3
}
jointPositionLimits
{
  ; activate constraint
  activate true 

  ; relaxed log barrier mu
  mu      0.01

  ; relaxed log barrier delta
  delta   1e-3
}

jointVelocityLimits
{
  ; relaxed log barrier mu
  mu      0.01

  ; relaxed log barrier delta
  delta   1e-3

  lowerBound
  {
    ; velocity limits for the different types of arm base DOFs
    base
    {
      (0,0)  -0.2 ; forward velocity
      (1,0)  -0.2 ; turning velocity
      (2,0)  -0.3 ; turning velocity
    }

    ; velocity limits for the arm DOFs
    arm
    {
      (0,0)  -0.2    ; torso_lift_joint [m/s]
      (1,0)  -0.3    ; r_shoulder_pan_joint [rad/s]
      (2,0)  -0.3	   ; r_shoulder_lift_joint [rad/s]
      (3,0)  -0.3	   ; r_upper_arm_roll_joint [rad/s]
      (4,0)  -0.3	   ; r_elbow_flex_joint [rad/s]
      (5,0)  -0.3	   ; r_forearm_roll_joint [rad/s]
      (6,0)  -0.2    ; torso_lift_joint [m/s]
      (7,0)  -0.3    ; r_shoulder_pan_joint [rad/s]
      (8,0)  -0.3	   ; r_shoulder_lift_joint [rad/s]
      (9,0)  -0.3	   ; r_upper_arm_roll_joint [rad/s]
      (10,0)  -0.3	   ; r_elbow_flex_joint [rad/s]
      (11,0)  -0.3	   ; r_forearm_roll_joint [rad/s]
    }
  }
  upperBound
  {
    ; velocity limits for the different types of arm base DOFs
    base
    {
      (0,0)  0.2 ; forward velocity
      (1,0)  0.2 ; turning velocity
      (2,0)  0.3 ; turning velocity
    }

    ; velocity limits for the arm DOFs
    arm
    {
      (0,0)  0.200     ; torso_lift_joint [m/s]
      (1,0)  0.30     ; r_shoulder_pan_joint [rad/s]
      (2,0)  0.30	   ; r_shoulder_lift_joint [rad/s]
      (3,0)  0.3000	   ; r_upper_arm_roll_joint [rad/s]
      (4,0)  0.3000	   ; r_elbow_flex_joint [rad/s]
      (5,0)  0.300	   ; r_forearm_roll_joint [rad/s]
      (6,0)  0.200     ; torso_lift_joint [m/s]
      (7,0)  0.30     ; r_shoulder_pan_joint [rad/s]
      (8,0)  0.300	   ; r_shoulder_lift_joint [rad/s]
      (9,0)  0.3000	   ; r_upper_arm_roll_joint [rad/s]
      (10,0)  0.3000	   ; r_elbow_flex_joint [rad/s]
      (11,0)  0.3000	   ; r_forearm_roll_joint [rad/s]
    }
  }
}


